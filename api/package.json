{
  "name": "linear-discord-api",
  "module": "server.ts",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "bun run build && NODE_ENV=development node --inspect dist/src/server.js",
    "dev": "nodemon -L --ext ts --exec \"bun run build-dev\"",
    "start": "bun run build && NODE_ENV=production node dist/server.js",
    "test": "NODE_ENV=test jest --setupFiles dotenv/config --runInBand",
    "lint": "eslint .",
    "reset:npm": "rm -rf node_modules && bun install"
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bignumber.js": "^9.1.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@swc/types": "^0.1.7",
    "@types/bun": "latest",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "bun-types": "^1.1.10",
    "eslint": "^9.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.3",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}